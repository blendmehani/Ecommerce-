										<span></span>
										<%=subcategory[i].Subcategory%>
										<% var count=0%>
										<%for(let j=0;j<products.length;j++){%>
										<%if(products[j].Subcategory==subcategory[i].Subcategory)
											count++%>
										<%}%>
										<small><%=count%></small>
									</label>
								</div>
							<%}%>
							</div>
						</div>
						<!-- /aside Widget -->

						
